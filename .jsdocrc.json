{
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "README.md",
            "lib/wrapper.js",
            "lib/list.js",
            "lib/hash.js",
            "lib/set.js"
        ]
    },
    "opts": {
        "template": "./node_modules/braintree-jsdoc-template",
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true
    },
    "templates": {
        "referenceTitle": "redis-type API",
        "disableSort": true,
        "collapse": false
    }
}
